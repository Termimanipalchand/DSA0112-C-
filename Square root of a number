#include <iostream>
#include <cmath> 
using namespace std;
int main() {
    double number, squareRoot;
    cout << "Enter a number: ";
    cin >> number;
    if (number >= 0) {
        squareRoot = sqrt(number);
        cout << "The square root of " << number << " is: " << squareRoot << endl;
    } else {
        cout << "The square root of a negative number is not defined in real numbers." << endl;
    }
    return 0;
}
